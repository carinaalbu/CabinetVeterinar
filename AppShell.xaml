<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CabinetVeterinar.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CabinetVeterinar"
    Shell.FlyoutBehavior="Disabled"
    Title="CabinetVeterinar">

    <TabBar>
        <ShellContent
             Title="Cabinet veterinar"
             ContentTemplate="{DataTemplate local:MainPage}"/>
        <ShellContent
             Title="About"
             ContentTemplate="{DataTemplate local:AboutPage}"/>
        <ShellContent Title="Programări" ContentTemplate="{DataTemplate local:ProgramariPage}" />
        <ShellContent
            Title="Animale"
            ContentTemplate="{DataTemplate local:PetsPage}" />
        <ShellContent
            Title="Notificări"
            ContentTemplate="{DataTemplate local:NotificationsPage}" />
    </TabBar>
</Shell>
